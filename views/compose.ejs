<%- include('partials/header'); -%>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="titleText" class="form-label">Title</label>
            <input type="text" name="titleText" class="form-control">
        </div>
        <div class="form-group">
            <label for="postText" class="form-label">Post</label>
            <textarea class="form-control" id="floatingTextarea" rows="5" cols="30"></textarea>
        </div>
        <button type=" submit" name="button" class=" btn btn-primary ">Publish</button>
    </form>
    <%- include('partials/footer'); -%>